    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^8.3.3",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^28.1.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.15.0",
    "he": "^1.2.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^28.0.3",
