{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "buildPlugins": "node plugins/plugin-compiler.js",
    "postinstall": "remix setup node",
    "deploy": "npm run build && flyctl deploy --verbose",
    "deploy:no-cache": "npm run build && flyctl deploy --no-cache",
    "deploy:staging": "npm run build && flyctl deploy -c fly-staging.toml",
    "startLocal": "node -r dotenv/config node_modules/.bin/remix-serve build",
    "start": "remix-serve build",
    "build": "npm run buildPlugins && npm run build:css && remix build",
    "build:css2": "tailwindcss -o app/styles/tailwind.css",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "tailwind": "npx tailwindcss -i ./styles/app.css -o ./app/styles/app.css --watch",
    "clean": "node clean.js",
    "graphCDN:build": "npx graphcdn push",
    "graphCDN:cacheResync": "node githubActions/clearGraphCDN.js",
    "dev": "npm run buildPlugins && run-p dev:*",
    "dev:css": "npm run css",
    "dev:remix": "remix dev",
    "css": "postcss styles --base styles --dir app/styles -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "test:clean": "npm run test -- --clearCache"
  },
  "dependencies": {
    "@apollo/client": "3.6.6",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@shopify/shopify-api": "^2.0.0",
    "@types/prettier": "^2.4.2",
    "@types/rss": "0.0.29",
    "canvas": "^2.8.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.1",
    "framer-motion": "^5.4.5",
    "graphql": "^16.0.1",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "prettier": "^2.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-imgix": "^9.5.1",
    "react-intersection-observer": "^8.33.1",
    "react-lazy-load-image-component": "1.5.4",
    "react-redux": "^7.2.6",
    "react-sticky": "^6.0.3",
    "remix": "^1.4.3",
