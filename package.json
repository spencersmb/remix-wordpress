{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "buildPlugins": "node plugins/plugin-compiler.js",
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "deploy": "npm run build && flyctl deploy --verbose",
    "deploy:no-cache": "npm run build && flyctl deploy --no-cache",
    "deploy:staging": "npm run build && flyctl deploy -c fly-staging.toml",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve build",
    "build": "npm run buildPlugins && npm run build:css && remix build",
    "build:css2": "tailwindcss -o app/styles/tailwind.css",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "npm run buildPlugins && concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "dev:cssOld": "tailwindcss --config ./tailwind.config.js -o app/styles/tailwind.css --watch",
    "tailwind": "npx tailwindcss -i ./styles/app.css -o ./app/styles/app.css --watch",
    "clean": "node clean.js",
    "graphCDN:build": "npx graphcdn push",
    "graphCDN:cacheResync": "node githubActions/clearGraphCDN.js"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@reduxjs/toolkit": "^1.6.2",
    "@remix-run/node": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@shopify/shopify-api": "^2.0.0",
    "@types/prettier": "^2.4.2",
    "@types/rss": "0.0.29",
    "canvas": "^2.8.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "dotenv": "^10.0.0",
    "framer-motion": "^5.4.5",
    "graphql": "^16.0.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "node-fetch": "^1.7.3",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.33.1",
    "react-redux": "^7.2.6",
    "remix": "^1.1.1",
    "remix-utils": "^2.2.0",
    "rss": "^1.2.2",
    "text-to-image": "^4.1.1",
    "text2png": "^2.3.0",
    "uuid": "^8.3.2",
    "xstate": "^4.26.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/lodash": "^4.14.177",
    "@types/md5": "^2.3.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.3",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "he": "^1.2.0",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
